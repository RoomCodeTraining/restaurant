{
    "name": "futurafric-ia/ciprel-cantine",
    "type": "project",
    "description": "Application de gestion de cantine.",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "blade-ui-kit/blade-icons": "^1.1",
        "essa/api-tool-kit": "^2.1",
        "fakerphp/faker": "^1.9.1",
        "filament/forms": "^3.0-stable",
        "filament/infolists": "^3.1",
        "filament/notifications": "^3.0-stable",
        "filament/tables": "^3.0-stable",
        "filament/widgets": "^3.1",
        "guzzlehttp/guzzle": "^7.2",
        "knuckleswtf/scribe": "^4.25",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "leandrocfe/filament-apex-charts": "^3.0",
        "livewire/livewire": "^3.2",
        "maatwebsite/excel": "^3.1",
        "psr/simple-cache": "2.0",
        "pxlrbt/filament-excel": "^2.1",
        "rap2hpoutre/laravel-log-viewer": "^2.2",
        "rappasoft/laravel-livewire-tables": "^3.1",
        "spatie/browsershot": "^3.52",
        "spatie/laravel-activitylog": "^4.4",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-model-states": "^2.1",
        "spatie/laravel-permission": "^5.3",
        "spatie/laravel-welcome-notification": "^2.2",
        "studio/laravel-totem": "^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.3",
        "laravel/breeze": "^1.4",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.26",
        "pestphp/pest-plugin-faker": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.2",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "php-cs-fixer": [
            "php-cs-fixer fix --config=.php-cs-fixer.php --cache-file=php-cs-fixer.cache ."
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
